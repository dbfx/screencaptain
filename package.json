{
  "name": "screencaptain",
  "productName": "ScreenCaptain",
  "version": "1.0.1",
  "description": "Free desktop screen recording for web developers.",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "sass": "node-sass sass/ -o css/"
  },
  "keywords": [],
  "author": {
    "name": "Dave Blakey",
    "email": "dave@blakey.co"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "appCopyright": "Copyright (C) 2020 Dave Blakey.",
        "icon": "images/icon.png"
      },
      "windowsStoreConfig": {
        "packageName": "screenCaptain"
      },
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "dbfx",
              "name": "screencaptain"
            },
            "prerelease": true
          }
        }
      ],
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "ScreenCaptain",
            "setupIcon": "images/icon.ico"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ],
          "config": {
            "name": "ScreenCaptain",
            "setupIcon": "images/icon.icns"
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "name": "ScreenCaptain",
            "setupIcon": "images/icon.png"
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {
            "name": "ScreenCaptain",
            "setupIcon": "images/icon.png"
          }
        }
      ]
    }
  },
  "dependencies": {
    "application-config": "^0.2.1",
    "bulma": "^0.8.2",
    "electron-squirrel-startup": "^1.0.0",
    "node-sass": "^4.14.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.50",
    "@electron-forge/maker-deb": "^6.0.0-beta.50",
    "@electron-forge/maker-rpm": "^6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.50",
    "@electron-forge/maker-zip": "^6.0.0-beta.50",
    "@electron-forge/publisher-github": "^6.0.0-beta.50",
    "autoprefixer": "^9.7.6",
    "css-loader": "^3.5.2",
    "electron": "9.4.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.4",
    "ts-loader": "^7.0.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
